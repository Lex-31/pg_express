---
const links = [
  { href: 'http://example1.com', text: 'Example 1' },
  { href: 'http://example2.com', text: 'Example 2' },
  { href: 'http://example3.com', text: 'Example 3' },
];
---

<nav>
  {links.map(link => (
    <a href={link.href} class="nav-link">{link.text}</a>
  ))}
</nav>

<style>
  nav {
    display: flex;
    gap: 1rem;
  }
  .nav-link {
    color: #fff;
    text-decoration: none;
    padding: 0.5rem 1rem;
    background-color: #007BFF;
    border-radius: 4px;
  }
</style>